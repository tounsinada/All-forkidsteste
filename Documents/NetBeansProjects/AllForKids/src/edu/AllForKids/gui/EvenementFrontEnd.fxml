<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.AllForKids.gui.EvenementFrontEndController">
   <children>
      <VBox layoutX="607.0" layoutY="24.0" prefHeight="461.0" prefWidth="212.0" spacing="15.0">
         <children>
            <HBox prefHeight="46.0" prefWidth="204.0" spacing="6.0">
               <children>
                  <TextField fx:id="Titre" onKeyPressed="#SearchByNom" prefHeight="25.0" prefWidth="143.0" promptText="chercher" style="-fx-border-color: #2471A3;" />
                  <Button fx:id="ChercherTitre" mnemonicParsing="false" onAction="#ChecherParNom" style="-fx-background-color: #22A7F0;" text="chercher" textFill="WHITE" />
               </children>
            </HBox>
            <Label text="Filtrer Par Type" />
            <CheckBox id="Randonnée" fx:id="Cultiver" mnemonicParsing="false" text="Pour se cultiver" />
            <CheckBox id="Voyage" fx:id="Distraire" mnemonicParsing="false" text="Pour se distraire" />
            <CheckBox id="Camping" fx:id="Cinema" mnemonicParsing="false" text="Cinema" />
            <CheckBox id="Marathon" fx:id="Rando" mnemonicParsing="false" text="Randonnée" />
            <CheckBox id="Autre" fx:id="autre" mnemonicParsing="false" text="Autre" />
            <Pane prefHeight="30.0" prefWidth="184.0">
               <children>
                  <Button layoutX="150.0" layoutY="4.0" mnemonicParsing="false" onAction="#Filtrer" prefHeight="25.0" prefWidth="61.0" style="-fx-background-color: #22A7F0;" text="filtrer" textFill="WHITE" />
               </children>
            </Pane>
            <Label text="Filtrer par date" />
            <DatePicker prefHeight="25.0" prefWidth="201.0" promptText="Date Debut" style="-fx-border-color: #2471A3;" />
            <DatePicker prefHeight="25.0" prefWidth="204.0" promptText="Date Fin" style="-fx-border-color: #2471A3;" />
            <HBox alignment="CENTER_RIGHT" prefHeight="26.0" prefWidth="184.0">
               <children>
                  <Button alignment="BOTTOM_RIGHT" mnemonicParsing="false" onAction="#FiltrerParDate" style="-fx-background-color: #22A7F0;" text="Chercher" textFill="WHITE" />
               </children>
            </HBox>
         </children>
      </VBox>
      <ScrollPane fx:id="LisTEvent" fitToHeight="true" fitToWidth="true" layoutX="12.0" layoutY="11.0" pannable="true" prefHeight="480.0" prefWidth="584.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ScrollPane>
   </children>
